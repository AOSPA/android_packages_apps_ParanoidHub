<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Paranoid Android Updates</string>
    <string name="app_preferences_name">Update preferences</string>
    <string name="os_name">Paranoid Android</string>
    <string name="join_two_unrelated_items"><xliff:g id="first_item">%1$s</xliff:g>, <xliff:g id="second_item">%2$s</xliff:g></string>

    <string name="app_preferences_class">com.paranoid.hub.HubPreferencesActivity$HubSettingsFragment</string>

    <!-- Directory where the update files will be downloaded and stored.
         WARNING: The application can and will delete any unknown file. -->
    <string name="download_path" translatable="false">/data/hub_updates/</string>

    <!-- Directory where the downloads will be exported to.
         The path is relative to the root of the external storage.-->
    <string name="export_path" translatable="false">ParanoidHub/</string>

    <!-- The URL of the devices json update file on the server -->
    <string name="updater_server_url" translatable="false">"https://raw.githubusercontent.com/Sirhc95/ota_test/master/%s.json"</string>

    <!-- The URL of the json that holds the updates information -->
    <string name="updater_changelog_url" translatable="false">"https://raw.githubusercontent.com/Sirhc95/ota_test/master/changelog.json"</string>

    <string name="menu_refresh">Refresh</string>
    <string name="menu_preferences">Preferences</string>

    <string name="dialog_export_title">Exporting update</string>
    <string name="notification_export_success">Update exported</string>
    <string name="notification_export_fail">Export error</string>
    <string name="toast_already_exporting">Already exporting an update</string>

    <plurals name="eta_seconds">
        <item quantity="one">1 second left</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> seconds left</item>
    </plurals>
    <plurals name="eta_minutes">
        <item quantity="one">1 minute left</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> minutes left</item>
    </plurals>
    <plurals name="eta_hours">
        <item quantity="one">1 hour left</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> hours left</item>
    </plurals>

    <string name="export_channel_title">Export completion</string>
    <string name="new_updates_channel_title">New updates</string>
    <string name="ongoing_channel_title">Ongoing downloads</string>
    <string name="update_failed_channel_title">Update failed</string>

    <!-- Up To Date -->
    <string name="no_updates_title">Your System is Up To Date</string>
    <string name="no_updates_text">Version: %1$s %2$s \nLast checked: %3$s</string>
    <string name="no_updates_last_checked">Last checked: %1$s</string>
    <string name="no_updates_current_version">Version: %1$s %2$s</string>
    <string name="no_updates_button_text">Check for update</string>
    <string name="no_updates_found_snack">No new updates found</string>

    <!-- Checking for update -->
    <string name="update_checking_title">Checking For Update...</string>

    <!-- Update available -->
    <string name="update_found_title">System Update Available</string>
    <string name="update_found_subtitle">%1$s %2$s</string>
    <string name="update_found_text">Paranoid Android %1$s %2$s \nUpdate size: %3$s</string>
    <string name="update_found_changelog_title">What\'s new</string>
    <string name="update_found_changelog">%1$s</string>
    <string name="update_found_size">Update size: %1$s</string>
    <string name="update_found_button_text">Download update</string>
    <string name="update_found_warning_and_desc">Downloading updates over a cellular network or while roaming may cause additional charges. \n\nThis update requires your device to restart</string>
    <string name="update_found_notification_title">System Update Available</string>
    <string name="update_found_notification_text">Paranoid Android %1$s %2$s</string>
    <string name="update_on_mobile_data_title">Warning</string>
    <string name="update_on_mobile_data_message">You\'re about to download an update package using mobile data which is likely going to cause high data usage. Would you like to proceed?</string>
    <string name="update_found_snack">New updates found</string>
    <string name="update_found_notification_snooze">Later</string>

    <!-- Update starting -->
    <string name="starting_update_notification_title">Starting Update</string>
    <string name="starting_update_notification_text">Preparing to download the update</string>

    <!-- Downloading update -->
    <string name="downloading_title">Downloading Update...</string>
    <string name="downloading_text">Downloadingâ€¦ %1$s\nTap on the button below to cancel downloading</string>
    <string name="downloading_paused_title">Download Paused</string>
    <string name="downloading_button_text">Cancel update</string>
    <string name="downloading_button_text_pause">Pause update</string>
    <string name="downloading_button_text_resume">Resume update</string>
    <string name="downloading_notification_title">Downloading Update</string>
    <string name="downloading_notification_text">%1$s, %2$s/s</string>
    <string name="downloading_donut_text_completed">completed</string>

    <!-- Update paused -->
    <string name="paused_update_notification_title">Download Paused</string>
    <string name="paused_update_notification_text">Resume at anytime</string>
    <string name="paused_error_update_notification_title">Problem pausing update</string>
    <string name="paused_error_update_notification_text">Start the download again when you\'re ready</string>

    <!-- Verifying update -->
    <string name="verifying_update_title">Verifying Update...</string>
    <string name="verifying_update_notification_title">@string/verifying_update_title</string>
    <string name="verifying_update_notification_text">This won\'t take long</string>
    <string name="verified_update_notification_title">Update Verified</string>
    <string name="verified_update_notification_text">Install the update when you\'re ready</string>
    <string name="verifying_error_update_notification_title">Problem verifying update</string>
    <string name="verifying_error_update_notification_text">Try downloading the update again</string>

    <!-- Verified update -->
    <string name="verified_download_snack">Update verified</string>

    <!-- Install -->
    <string name="install_title">Apply Update</string>
    <string name="install_button_text">Install update</string>
    <string name="install_warning_and_desc">Tap the restart button below to apply the update. This process can take 5-10 minutes.</string>
    <string name="install_warning_and_desc_ab">While the update is being applied, you can use your phone as normal. \nWhen it\'s finished, tap the restart button to finalize the update.</string>
    <string name="install_update_notification_title">Ready to update</string>
    <string name="install_update_notification_text">Tap to finish applying the update</string>
    <string name="install_low_battery_warning_title">Low battery</string>
    <string name="install_low_battery_warning_text">The battery level is too low, you need at least <xliff:g id="percent_discharging">%1$d</xliff:g>%% of the battery to continue, <xliff:g id="percent_charging">%2$d</xliff:g>%% if charging.</string>
    <string name="install_update_dialog_title">Apply update</string>
    <string name="install_update_dialog_message">You are about to upgrade to <xliff:g id="update_name">%1$s</xliff:g>.\n\nIf you press <xliff:g id="ok">%2$s</xliff:g>, the device will restart itself in recovery mode to install the update.\n\nNote: This feature requires a compatible Recovery or updates will need to be installed manually.</string>
    <string name="install_update_dialog_message_ab">You are about to upgrade to <xliff:g id="update_name">%1$s</xliff:g>.\n\nIf you press <xliff:g id="ok">%2$s</xliff:g>, the device will begin installing in the background.\n\nOnce completed, you will be prompted to reboot.</string>
    <string name="install_update_dialog_message_info">%1$s %2$s (%3$s)</string>
    <string name="installing_title">Applying Update</string>
    <string name="installing_warning_and_desc_ab">The update is being applied, do not power off your device during this process. \nYou can suspend the update by tapping suspend.</string>
    <string name="installing_suspended_text">Update Suspended</string>
    <string name="installing_update_notification_title">Preparing Update</string>
    <string name="installing_update_notification_title_ab">Applying Update</string>
    <string name="installing_update_notification_text">Tap to read more</string>
    <string name="installing_error_update_notification_title">Installation Failed</string>
    <string name="installing_error_update_notification_text">Tap to read more</string>
    <string name="installing_suspended_update_notification_title">@string/installing_suspended_text</string>
    <string name="installing_suspended_update_notification_text">Resume the installation at any time</string>
    <string name="installing_suspend_button_text">Suspend</string>
    <string name="installing_suspended_button_text">@string/downloading_button_text_resume</string>
    <string name="installing_suspended_warning_and_desc_ab">The update is currently suspended, you can resume the installation at anytime.</string>
    <string name="installing_finalizing_update_notification_text">Finalizing installation</string>
    <string name="installing_preparing_update_notification_text">Preparing for next boot</string>
    <string name="installing_cancel_dialog_message">Cancel the installation?</string>
    <string name="installed_title">Update Applied</string>
    <string name="installed_text">Update successfully installed. Restart your device to apply</string>
    <string name="installed_button_text">Restart device</string>
    <string name="installed_update_notification_title">Update Applied</string>
    <string name="installed_update_notification_text">Please restart you\'re device</string>

    <!-- Restart -->
    <string name="restart_title">Restart to apply</string>
    <string name="restart_button_text">@string/installed_button_text</string>

    <!-- Errors -->
    <string name="error_update_check_failed_snack">The update check failed. Please check your internet connection and try again later.</string>
    <string name="error_download_failed_snack">The download failed. Please check your internet connection and try again later.</string>
    <string name="error_verification_failed_snack">The update verification failed.</string>
    <string name="error_update_not_installable_snack">This update can\'t be installed on top of the current build.</string>

    <!-- Preferences -->
    <string name="auto_delete_updates_title">Auto delete updates</string>
    <string name="auto_delete_updates_summary">Automatically delete updates after they are installed</string>
    <string name="ab_perf_mode_title">Performance mode</string>
    <string name="ab_perf_mode_summary">Speed\'s up the installation process</string>
    <string name="allow_downgrading_title">Allow downgrading</string>
    <string name="allow_downgrading_summary">Allow\'s installing older system versions</string>
</resources>
